<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ca.mas.push">

    <application>
        <service
            android:name=".MASFirebaseInstanceIdService"
            android:enabled="true"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <provider
            android:name=".MASPushContentProvider"
            android:authorities="com.ca.mas.push"
            android:enabled="true"
            android:exported="false"
            android:initOrder="200" >
            <meta-data android:name="grant-type" android:value="@integer/client_credential_grant_flow"/>
            <meta-data android:name="register-on-start" android:value="@bool/register_on_start"/>
        </provider>


   </application>

</manifest>